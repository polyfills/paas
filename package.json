{
  "name": "paas",
  "description": "Polyfill as a Service",
  "version": "2.1.0",
  "author": "Jonathan Ong <me@jongleberry.com> (http://jongleberry.com)",
  "license": "MIT",
  "repository": "polyfills/paas",
  "dependencies": {
    "koa": "^0.12.2",
    "polyfills": "^1.7.0",
    "polyfills-db": "^1.10.0"
  },
  "devDependencies": {
    "istanbul-harmony": "0",
    "mocha": "1",
    "supertest": "^0.14.0"
  },
  "scripts": {
    "install": "npm dedupe",
    "start": "node --harmony-generators .",
    "test": "mocha --harmony-generators --reporter spec --timeout 10s",
    "test-cov": "node --harmony-generators node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --reporter dot --timeout 10s",
    "test-travis": "node --harmony-generators node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- --reporter dot --timeout 10s"
  },
  "main": "lib",
  "engines": {
    "node": ">= 0.11.14"
  },
  "keywords": [
    "polyfill",
    "polyfills",
    "service"
  ]
}
